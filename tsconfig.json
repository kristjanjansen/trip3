{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowJs": true,
        "checkJs": false,
        "lib": ["ESNext", "DOM"],
        "types": ["vite/client", "ziggy-js"]
    },
    "exclude": ["node_modules"]
}
