{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowJs": true,
        "checkJs": false,
        "lib": ["esnext", "dom"],
        "types": ["vite/client", "ziggy-js"]
    },
    "exclude": ["node_modules"]
}
